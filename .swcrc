{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es2015"
  },
  "module": {
    "type": "amd",
    "moduleRoot": "src"
  },
  "sourceMaps": true,
  "env": {
    "exclude": [
      "transform-regenerator"
    ]
  }
}
